<div class="container">
    <h1>Log in</h1>
    <div class="form-group row">
        <%= form_tag '/login' do %>

      <p style="margin-left:29px"><%= label_tag :email %>
        <%= text_field_tag :email %></p>

      <p><%= label_tag :password %>
        <%= password_field_tag :password %></p>

        <%= submit_tag "Submit", class: "btn btn-primary navbar-wrapper", style: "margin-left:150px" %>

        <% end %>
    </div>
</div>
